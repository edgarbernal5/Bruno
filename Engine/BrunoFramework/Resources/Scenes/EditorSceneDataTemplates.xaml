<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
                    xmlns:local="clr-namespace:BrunoFramework.Editor.Game"
                    xmlns:editor="clr-namespace:BrunoFramework.Editor.Units">

    <ItemContainerTemplate DataType="{x:Type local:SceneDocumentViewModel}">
        <local:SceneView />
    </ItemContainerTemplate>

    <ItemContainerTemplate DataType="{x:Type editor:SnapSettingsViewModel}">
        <editor:SnapSettingsView />
    </ItemContainerTemplate>

</ResourceDictionary>